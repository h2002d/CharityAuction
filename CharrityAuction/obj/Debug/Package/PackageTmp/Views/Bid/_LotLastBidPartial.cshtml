@model List<CharrityAuction.Models.BidModel>
<table id="bids">
    <tr>
        <th class="color-blue">@Resource.Buyer</th>
        <th class="color-blue">@Resource.LotValue</th>
        <th colspan="2" class="color-blue">@Resource.LotDate</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="last-bids-row">
            <td>@item.User.Nickname</td>
            <td>@item.Amount</td>
            <td>@item.CreateDate.ToString("hh:mm tt", System.Globalization.CultureInfo.InvariantCulture)</td>
            <td>@item.CreateDate.ToString("dd/MM/yyyy", System.Globalization.CultureInfo.InvariantCulture)</td>

        </tr>
    }
</table>